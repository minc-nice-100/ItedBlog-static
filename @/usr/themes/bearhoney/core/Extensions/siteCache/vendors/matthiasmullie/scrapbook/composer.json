{
  "name": "matthiasmullie/scrapbook",
  "type": "library",
  "description": "Scrapbook is a PHP cache library, with adapters for e.g. Memcached, Redis, Couchbase, APCu, SQL and additional capabilities (e.g. transactions, stampede protection) built on top.",
  "keywords": [
    "cache", "caching",
    "key", "value", "store",
    "apc",
    "memcached",
    "redis",
    "couchbase",
    "sql", "mysql", "sqlite", "postgresql",
    "filesystem", "flysystem",
    "buffer", "buffered",
    "transaction", "transactional", "commit", "rollback",
    "stampede", "protection", "mitigation",
    "psr-6", "psr-cache",
    "psr-16", "psr-simple-cache"
  ],
  "homepage": "https://scrapbook.cash",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthias Mullie",
      "homepage": "https://www.mullie.eu",
      "email": "scrapbook@mullie.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "psr/cache": "^2.0||^3.0",
    "psr/simple-cache": "^2.0||^3.0"
  },
  "require-dev": {
    "ext-pcntl": "*",
    "friendsofphp/php-cs-fixer": ">=2.0",
    "phpunit/phpunit": ">=10.0",
    "squizlabs/php_codesniffer": ">=3.0"
  },
  "suggest": {
    "ext-apcu": ">=4.0.0",
    "ext-couchbase": ">=3.0.0",
    "ext-memcached": ">=2.0.0",
    "ext-redis": ">=2.2.0||0.0.0.0",
    "ext-pdo": ">=0.1.0",
    "couchbase/couchbase": ">=3.0",
    "league/flysystem": ">=1.0"
  },
  "provide": {
    "psr/cache-implementation": "^1.0||^2.0||^3.0",
    "psr/simple-cache-implementation": "^1.0||^2.0||^3.0"
  },
  "autoload": {
    "psr-4": {
      "MatthiasMullie\\Scrapbook\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MatthiasMullie\\Scrapbook\\Tests\\": "tests/"
    }
  }
}
